extends layout

block main-content
  - let branchName = library.querySelector("name").textContent;  
  - let branchAdress = library.querySelector("address").textContent;
  - let phoneNum = library.querySelector("phoneNumber").textContent; 
  - let description = library.querySelector("description").textContent;
  h1.page-title 
  h2 #{branchName}
  p #{branchAdress}
  p #{phoneNum}
  p #{description}